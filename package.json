{
    "name": "dj-bot",
    "version": "1.0.0",
    "description": "Dj Bot pour discord",
    "main": "src/main.ts",
    "author": "vdeleeuw",
    "scripts": {
        "start": "node ./dist/src/main.js",
        "build": "tsc --build",
        "dev": "eslint && npm run build && npm run start",
        "prod": "npm run build && node ./dist/src/main.js",
        "esfix": "eslint --fix --ext .js,.ts",
        "prettier": "prettier --write \"{,src/**/}*.{md,json,ts}\""
    },
    "engines": {
        "node": ">=16.11.0"
    },
    "dependencies": {
        "@discordjs/voice": "^0.16.0",
        "axios": "^1.4.0",
        "discord.js": "^14.11.0",
        "ffmpeg-static": "^5.1.0",
        "i18n": "^0.15.1",
        "play-dl": "^1.9.7",
        "string-progressbar": "^1.0.4",
        "youtube-sr": "^4.3.0"
    },
    "optionalDependencies": {
        "@discordjs/opus": "^0.9.0",
        "libsodium-wrappers": "^0.7.13",
        "sodium-native": "^3.4.1"
    },
    "devDependencies": {
        "@types/i18n": "^0.13.6",
        "@types/node": "^20.3.1",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.9.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsdoc": "^46.4.5",
        "eslint-plugin-prefer-arrow": "^1.2.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.1"
    }
}
